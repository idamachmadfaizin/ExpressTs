{
	"name": "express-ts",
	"version": "1.0.0",
	"description": "The ExpressJs + Typescript + Webpack starter project",
	"scripts": {
		"start": "nodemon",
		"build": "build:webpack:prod",
		"build:ts": "tsc --build --clean & tsc --build",
		"build:webpack:dev": "cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" && webpack --mode=development",
		"build:webpack:prod": "cross-env TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack --mode=production --node-env=production",
		"dev": "node dist/webpack",
		"lint": "eslint src"
	},
	"keywords": [
		"express",
		"expressjs",
		"expressts",
		"expressjs typescript",
		"typescript",
		"webpack"
	],
	"author": "Idamachmadfaizin",
	"license": "MIT",
	"dependencies": {
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"helmet": "^6.0.1",
		"hpp": "^0.2.3",
		"http-status-codes": "^2.2.0",
		"morgan": "^1.10.0",
		"serve-favicon": "^2.5.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^4.6.0"
	},
	"devDependencies": {
		"@babel/preset-typescript": "^7.18.6",
		"@types/compression": "^1.7.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/hpp": "^0.2.2",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.13.0",
		"@types/serve-favicon": "^2.5.3",
		"@types/swagger-jsdoc": "^6.0.1",
		"@types/swagger-ui-express": "^4.1.3",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^5.51.0",
		"@typescript-eslint/parser": "^5.51.0",
		"@webpack-cli/generators": "^3.0.1",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.33.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.27.5",
		"nodemon": "^2.0.20",
		"prettier": "^2.8.4",
		"snyk": "^1.1100.0",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.2",
		"typescript": "^4.9.5",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"webpack-node-externals": "^3.0.0"
	}
}
